@article{holSpatialStructureFacilitates2013,
  title = {Spatial {{Structure Facilitates Cooperation}} in a {{Social Dilemma}}: {{Empirical Evidence}} from a {{Bacterial Community}}},
  author = {Hol, Felix J.H. and Galajda, Peter and Nagy, Krisztina and Woolthuis, Rutger G. and Dekker, Cees and Keymer, Juan E.},
  date = {2013},
  journaltitle = {PLoS ONE},
  volume = {8},
  number = {10},
  eprint = {24167557},
  eprinttype = {pmid},
  pages = {2--11},
  issn = {19326203},
  doi = {10.1371/journal.pone.0077042},
  abstract = {Cooperative organisms are ubiquitous in nature, despite their vulnerability to exploitation by cheaters. Although numerous theoretical studies suggest that spatial structure is critical for cooperation to persist, the spatial ecology of microbial cooperation remains largely unexplored experimentally. By tracking the community dynamics of cooperating (rpoS wild-type) and cheating (rpoS mutant) Escherichia coli in well-mixed flasks and microfabricated habitats, we demonstrate that spatial structure stabilizes coexistence between wild-type and mutant and thus facilitates cooperator maintenance. We develop a method to interpret our experimental results in the context of game theory, and show that the game wild-type and mutant bacteria play in an unstructured environment changes markedly over time, and eventually obeys a prisoner's dilemma leading to cheater dominance. In contrast, when wild-type and mutant E. coli co-inhabit a spatially-structured habitat, cooperators and cheaters coexist at intermediate frequencies. Our findings show that even in microhabitats lacking patchiness or spatial heterogeneities in resource availability, surface growth allows cells to form multi-cellular aggregates, yielding a self-structured community in which cooperators persist.},
  isbn = {1932-6203},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\CA24UT9A\\journal.pone.0077042.PDF;C\:\\Users\\Jonathan\\Zotero\\storage\\JJAAZSSB\\pone.0077042.pdf}
}

