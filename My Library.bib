  keywords = {Adaptation,Alcaligenes,Alcaligenes: growth & development,Alcaligenes: metabolism,Bacteriological Techniques,Biodegradation,Environmental,Environmental Pollutants,Environmental Pollutants: metabolism,Naphthalenes,Naphthalenes: metabolism,Physiological,Pseudomonas putida,Pseudomonas putida: growth & development,Pseudomonas putida: metabolism,Xenobiotics,Xenobiotics: metabolism},
