  keywords = {Culture Media,Deuterium,Kinetics,NAD,NADP,Naphthalenes,Naphthalenes: metabolism,Oxidation-Reduction,Oxygen Consumption,Oxygenases,Oxygenases: metabolism,Pseudomonas,Pseudomonas: metabolism,Stereoisomerism},
