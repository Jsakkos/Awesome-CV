  keywords = {Adsorption,Chlorophenols,GAC,Isotherms,Modeling,Phenol},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\3DZL2Q7B\\2007-Modeling_of_adsorption_isotherms_of_phenol_and_chlorophenols_onto_granular_activated_carbon._Part_I._Two-parameter_.pdf}
}

@article{hammarSinglecellScreeningPhotosynthetic2015,
