  author = {Mutalik, Vivek K. and Guimaraes, Joao C. and Cambray, Guillaume and Mai, Quynh Anh and Christoffersen, Marc Juul and Martin, Lance and Yu, Ayumi and Lam, Colin and Rodriguez, Cesar and Bennett, Gaymon and Keasling, Jay D. and Endy, Drew and Arkin, Adam P.},
