@article{Dickson2012,
  title = {Transcriptomic Responses of {{{\emph{Synechocystis}}}}{\emph{ Sp. }} {{PCC}} 6803 Encapsulated in Silica Gel.},
  author = {Dickson, David J and Luterra, Markael D and Ely, Roger L},
