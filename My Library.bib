@article{Manzano2013,
  title = {Quantification of Complex Polycyclic Aromatic Hydrocarbon Mixtures in Standard Reference Materials Using Comprehensive Two-Dimensional Gas Chromatography with Time-of-Flight Mass Spectrometry.},
  author = {Manzano, Carlos and Hoh, Eunha and Simonich, Staci L Massey},
  date = {2013-09-13},
  journaltitle = {Journal of chromatography. A},
  volume = {1307},
  eprint = {23932031},
