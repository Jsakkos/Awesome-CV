@article{Loh1984,
  title = {Use of the Fluorescent Probe 1-{{N-phenylnaphthylamine}} to Study the Interactions of Aminoglycoside Antibiotics with the Outer Membrane of {{Pseudomonas}} Aeruginosa.},
  author = {Loh, B and Grant, C and Hancock, R E},
