@article{fanChromosomefreeBacterialCells2020,
  title = {Chromosome-Free Bacterial Cells Are Safe and Programmable Platforms for Synthetic Biology},
  author = {Fan, Catherine and Davison, Paul A. and Habgood, Robert and Zeng, Hong and Decker, Christoph M. and Salazar, Manuela Gesell and Lueangwattanapong, Khemmathin and Townley, Helen E. and Yang, Aidong and Thompson, Ian P. and Ye, Hua and Cui, Zhanfeng and Schmidt, Frank and Neil Hunter, C. and Huang, Wei E.},
