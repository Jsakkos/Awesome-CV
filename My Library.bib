@article{linBiotransformationHydroxymethylfurfuralFurandicarboxylic2020,
  title = {Biotransformation of 5‐{{Hydroxymethylfurfural}} to 2,5‐{{Furandicarboxylic Acid}} by a {{Syntrophic Consortium}} of {{Engineered Synechococcus Elongatus}} and {{Pseudomonas Putida}}},
