@article{Desmet2015,
  title = {Highly Efficient, Long Life, Reusable and Robust Photosynthetic Hybrid Core–Shell Beads for the Sustainable Production of High Value Compounds},
  author = {Desmet, Jonathan and Meunier, Christophe and Danloy, Emeric and Duprez, Marie-Eve and Lox, Frédéric and Thomas, Diane and Hantson, Anne-Lise and Crine, Michel and Toye, Dominique and Rooke, Joanna and Su, Bao-Lian},
