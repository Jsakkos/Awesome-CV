@article{Gobel2010,
  title = {S-Layer Templated Bioinspired Synthesis of Silica},
  author = {Göbel, Caren and Schuster, Bernhard and Baurecht, Dieter and Sleytr, Uwe B. and Pum, Dietmar},
  date = {2010},
  journaltitle = {Colloids and Surfaces B: Biointerfaces},
  volume = {75},
