@article{Liu2008,
  title = {From {{Langmuir}} Kinetics to First- and Second-Order Rate Equations for Adsorption},
  author = {Liu, Yu and Shen, Liang},
  date = {2008},
  journaltitle = {Langmuir},
  volume = {24},
  number = {20},
  eprint = {18788769},
