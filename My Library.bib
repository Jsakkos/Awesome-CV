@article{khattarTwoPolypeptideProducts1997,
  title = {Two Polypeptide Products of the {{Escherichia}} Coli Cell Division Gene {{ftsW}} and a Possible Role for {{ftsW}} in {{ftsZ}} Function},
  author = {Khattar, Medhat M. and Addinall, Stephen G. and Stedul, Kristina H. and Boyle, David S. and Lutkenhaus, Joe and Donachie, William D.},
