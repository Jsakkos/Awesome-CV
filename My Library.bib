  author = {Wang, Xin and Liu, Wei and Xin, Changpeng and Zheng, Yi and Cheng, Yanbing and Sun, Su and Li, Runze and Zhu, Xin-Guang and Dai, Susie Y. and Rentzepis, Peter M. and Yuan, Joshua S.},
