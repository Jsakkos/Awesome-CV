  keywords = {Analytical methods,Biocatalytic phosphorylation,Bioprocess design,Bioprocess optimization,C-phosphorylation,Catalytic asymmetric phosphorylation,Emerging biocatalytic phosphorylation reactions,Metabolite stability,Metabolites,N-phosphorylation,O-phosphorylation,Phosphate donors,Phosphorylated metabolites,Phosphorylating enzymes,Phosphorylation,Reaction engineering,Reaction kinetics,S-phosphorylation},
