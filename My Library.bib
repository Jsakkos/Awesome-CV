@article{Sikkema1995,
  title = {Mechanisms of Membrane Toxicity of Hydrocarbons},
  author = {Sikkema, J and de Bont, J A and Poolman, B},
  options = {useprefix=true},
  date = {1995},
  journaltitle = {Microbiol Rev},
  volume = {59},
  number = {2},
  eprint = {7603409},
  eprinttype = {pmid},
  pages = {201--222},
  issn = {0146-0749},
  doi = {0146-0749/95/$04.00+0},
  abstract = {Microbial transformations of cyclic hydrocarbons have received much attention during the past three decades. Interest in the degradation of environmental pollutants as well as in applications of microorganisms in the catalysis of chemical reactions has stimulated research in this area. The metabolic pathways of various aromatics, cycloalkanes, and terpenes in different microorganisms have been elucidated, and the genetics of several of these routes have been clarified. The toxicity of these compounds to microorganisms is very important in the microbial degradation of hydrocarbons, but not many researchers have studied the mechanism of this toxic action. In this review, we present general ideas derived from the various reports mentioning toxic effects. Most importantly, lipophilic hydrocarbons accumulate in the membrane lipid bilayer, affecting the structural and functional properties of these membranes. As a result of accumulated hydrocarbon molecules, the membrane loses its integrity, and an increase in permeability to protons and ions has been observed in several instances. Consequently, dissipation of the proton motive force and impairment of intracellular pH homeostasis occur. In addition to the effects of lipophilic compounds on the lipid part of the membrane, proteins embedded in the membrane are affected. The effects on the membrane-embedded proteins probably result to a large extent from changes in the lipid environment; however, direct effects of lipophilic compounds on membrane proteins have also been observed. Finally, the effectiveness of changes in membrane lipid composition, modification of outer membrane lipopolysaccharide, altered cell wall constituents, and active excretion systems in reducing the membrane concentrations of lipophilic compounds is discussed. Also, the adaptations (e.g., increase in lipid ordering, change in lipid/protein ratio) that compensate for the changes in membrane structure are treated.},
  isbn = {0146-0749 (Print)\textbackslash r0146-0749 (Linking)},
  keywords = {Bacteria/*drug effects,Cell Membrane/*drug effects,Fungi/*drug effects,Hydrocarbons/*pharmacology},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\55HRNA6E\\1995-Mechanisms_of_membrane_toxicity_of_hydrocarbons.pdf}
}

