@article{koleyDiscoveryBiofilmElectrocline2011,
  title = {Discovery of a Biofilm Electrocline Using Real-Time {{3D}} Metabolite Analysis},
  author = {Koley, Dipankar and Ramsey, Matthew M and Bard, Allen J and Whiteley, Marvin},
