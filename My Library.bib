@article{vanhammeInfluenceChemicalSurfactants1999,
  title = {Influence of Chemical Surfactants on the Biodegradation of Crude Oil by a Mixed Bacterial Culture},
  author = {Van Hamme, J D and Ward, O P},
  date = {1999},
  journaltitle = {Canadian Journal of Microbiology},
  volume = {45},
  number = {2},
