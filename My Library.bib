@article{camsundDesignAnalysisLacIrepressed2014,
  title = {Design and Analysis of {{LacI-repressed}} Promoters and {{DNA-looping}} in a Cyanobacterium},
  author = {Camsund, Daniel and Heidorn, Thorsten and Lindblad, Peter},
  date = {2014},
  journaltitle = {Journal of Biological Engineering},
