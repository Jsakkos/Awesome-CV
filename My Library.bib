@article{dongElevatedATPaseActivity2010,
  title = {Elevated {{ATPase Activity}} of {{KaiC Applies}} a {{Circadian Checkpoint}} on {{Cell Division}} in {{Synechococcus}} Elongatus},
  author = {Dong, Guogang and Yang, Qiong and Wang, Qiang and Kim, Yong Ick and Wood, Thammajun L. and Osteryoung, Katherine W. and van Oudenaarden, Alexander and Golden, Susan S.},
  options = {useprefix=true},
  date = {2010},
  journaltitle = {Cell},
  volume = {140},
