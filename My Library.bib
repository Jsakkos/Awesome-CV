@article{liIsobutanolProductionAlternative2014,
  title = {Isobutanol Production as an Alternative Metabolic Sink to Rescue the Growth Deficiency of the Glycogen Mutant of {{Synechococcus}} Elongatus {{PCC}} 7942},
