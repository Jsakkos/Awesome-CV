@article{Park2004,
  title = {Survival of Naphthalene-Degrading {{Pseudomonas}} Putida {{NCIB}} 9816-4 in Naphthalene-Amended Soils: Toxicity of Naphthalene and Its Metabolites.},
  author = {Park, W and Jeon, C O and Cadillo, H and DeRito, C and Madsen, E L},
