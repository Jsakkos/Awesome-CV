@article{Herbold1998,
  title = {Studies on the Effect of the Solvents Dimethylsulfoxide and Ethyleneglycoldimethylether on the Mutagenicity of Four Types of Diisocyanates in the {{Salmonella}}/Microsome Test.},
  author = {Herbold, B and Haas, P and Seel, K and Walber, U},
  date = {1998-01-30},
  journaltitle = {Mutation research},
  volume = {412},
  number = {2},
  eprint = {9539971},
  eprinttype = {pmid},
  pages = {167--75},
  issn = {0027-5107},
  abstract = {The mutagenicity of isomers and homologs of diphenylmethanediisocyanate (4,4'-diisocyanatodiphenylmethane, 2,4'-diisocyanatodiphenylmethane, a mixture of monomeric MDI isomers, and polymeric MDI), containing 55-100\% of monomeric MDI, was determined in the Salmonella/microsome test using dimethylsulfoxide (DMSO) and ethyleneglycoldimethylether (EGDE) as solvents. Positive results were obtained for DMSO solutions of all four diisocyanates in the presence of S9 mix containing 30\% S9 fraction. Uniformly negative results were found when the diisocyanates were dissolved in EDGE. These results correspond to those of analytical investigations. A small amount of diaminodiphenylmethane (MDA) is one of the reaction products formed when MDI is dissolved in commercial DMSO. No MDA could be detected in solutions of MDI in EGDE. It is therefore concluded that the positive results obtained with diisocyanates in DMSO solutions are due to the formation of MDA. This is artificially formed through the hydrolysis of MDI, caused by traces of water that are always present in DMSO. These findings indicate that DMSO is an inappropriate solvent and should therefore not be used in any in vitro study with diisocyanates. EGDE may be a suitable replacement. The positive test results reported so far for DMSO solutions of MDI are thus only of limited relevance for risk evaluation.},
  keywords = {Dimethyl Sulfoxide,Dimethyl Sulfoxide: pharmacology,Drug Interactions,Drug Stability,Ethyl Ethers,Ethyl Ethers: pharmacology,Isocyanates,Isocyanates: chemistry,Isocyanates: toxicity,Mutagenicity Tests,Mutagens,Mutagens: toxicity,Salmonella typhimurium,Salmonella typhimurium: drug effects,Salmonella typhimurium: genetics,Solvents,Solvents: pharmacology},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\UQE6AHUK\\1998-Studies_on_the_effect_of_the_solvents_dimethylsulfoxide_and_ethyleneglycoldimethylether_on_the_mutagenicity_of_four.pdf}
