  issn = {10974172},
  doi = {10.1016/j.cell.2019.05.055},
  abstract = {Multicellular lifestyle requires cell-cell connections. In multicellular cyanobacteria, septal junctions enable molecular exchange between sister cells and are required for cellular differentiation. The structure of septal junctions is poorly understood, and it is unknown whether they are capable of controlling intercellular communication. Here, we resolved the in situ architecture of septal junctions by electron cryotomography of cryo-focused ion beam-milled cyanobacterial filaments. Septal junctions consisted of a tube traversing the septal peptidoglycan. Each tube end comprised a FraD-containing plug, which was covered by a cytoplasmic cap. Fluorescence recovery after photobleaching showed that intercellular communication was blocked upon stress. Gating was accompanied by a reversible conformational change of the septal junction cap. We provide the mechanistic framework for a cell junction that predates eukaryotic gap junctions by a billion years. The conservation of a gated dynamic mechanism across different domains of life emphasizes the importance of controlling molecular exchange in multicellular organisms. The in situ architecture of cyanobacterial septal junctions reveals a gated intercellular communication channel that evolutionarily predates eukaryotic gap junctions by a billion years.},
  keywords = {cell-cell connections,cyanobacteria,electron cryotomography,fluorescence recovery after photobleaching,membrane trafficking,multicellularity,septal junctions,subtomogram averaging},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\X364IPEY\\1-s2.0-S0092867419306269-main.pdf}
}

@article{weissSyntheticLightdrivenConsortium2017,
  title = {A Synthetic, Light-Driven Consortium of Cyanobacteria and Heterotrophic Bacteria Enables Stable Polyhydroxybutyrate Production},
  author = {Weiss, Taylor L. and Young, Eric J. and Ducat, Daniel C.},
  date = {2017},
  journaltitle = {Metabolic Engineering},
