@article{karradtNblAKeyProtein2008,
  title = {{{NblA}}, a Key Protein of Phycobilisome Degradation, Interacts with {{ClpC}}, a {{HSP100}} Chaperone Partner of a Cyanobacterial {{Clp}} Protease},
