  author = {Riback, Joshua A. and Katanski, Christopher D. and Kear-Scott, Jamie L. and Pilipenko, Evgeny V. and Rojek, Alexandra E. and Sosnick, Tobin R. and Drummond, D. Allan},
  date = {2017-03-09},
