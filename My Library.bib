@article{Sing2013a,
  title = {Assessment of {{Microporosity}}},
  author = {Sing, Kenneth S W and Rouquerol, Françoise and Llewellyn, Philip and Rouquerol, Jean},
  date = {2013},
  journaltitle = {Adsorption by Powders and Porous Solids: Principles, Methodology and Applications: Second Edition},
  pages = {303--320},
  doi = {10.1016/B978-0-08-097035-6.00009-7},
  abstract = {The assessment of microporosity is discussed in this chapter together with the interpretation of Type I adsorption isotherms and microcalorimetric data. Two different mechanisms of micropore filling are characterized by adsorption microcalorimetry and isotherm analysis. Thus, physisorption in pores of molecular dimensions involves enhanced adsorbent-adsorbate interactions and distortion of the corresponding isotherms at very low p/po, whereas wider micropores are filled over a range of higher p/po. The application of the various procedures for isotherm analysis requires critical appraisal. Although the Dubinin-Stoeckli and Horvath-Kawazoe methods are still widely used for evaluating the micropore size distribution, density functional theory has a number of advantages and is now being actively developed and improved. This method can be applied to adsorption isotherms of nitrogen at 77K and argon at 87K, the latter adsorptive becoming increasingly attractive for surface area determination and pore size analysis. Useful information obtained by a calorimetric approach is also discussed. Immersion microcalorimetry is of special interest for several reasons: with various immersion liquids, one can probe the micropore sizes by molecular sieving, or simply carry out a rapid screening of a set of adsorbents. Gas adsorption microcalorimetry provides an independent way of studying micropore filling and characterizing the various types of micropores. © 2014 Elsevier Ltd All rights reserved.},
  isbn = {9780080970356},
  keywords = {Density functional theory,DFT,Dubinin-Radushkevich-Stoeckli methods,Horvath-Kawazoe method,Micropore surface area,Micropores,Molecular probe screening,Pore size distribution,QSDFT,T method,αs method},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\MKTXZ5LU\\2013-Assessment_of_Microporosity.pdf}
}

@book{Sing2013b,
  title = {Assessment of {{Mesoporosity}}},
  author = {Sing, Kenneth S W and Rouquerol, Françoise and Rouquerol, Jean and Llewellyn, Philip},
  date = {2013},
  journaltitle = {Adsorption by Powders and Porous Solids: Principles, Methodology and Applications: Second Edition},
  volume = {270},
  doi = {10.1016/B978-0-08-097035-6.00008-5},
  abstract = {The aim of this chapter is to discuss the problems involved in mesopore size analysis and hence in the interpretation of Type IV isotherms. First, the terms mesopore volume, porosity and hydraulic radius are defined, and their relevance is briefly explained. There follows an outline of the theory of capillary condensation and the application of the Kelvin equation. The 'classical' procedures for mesopore size analysis are then critically reviewed with particular attention given to the application of the popular Barrett, Joyner and Halenda (BJH) method. Next, a short account is given of the basic principles involved in the use of density functional theory (DFT). The relative advantages and limitations of the DFT and BJH methods are then considered together with the merits of using argon as the adsorptive at 87. K (instead of nitrogen at 77. K). The difficulties associated with physisorption hysteresis loops are also discussed. A few general conclusions and recommendations are given in the final section of this chapter. © 2014 Elsevier Ltd All rights reserved.},
  isbn = {978-0-08-097035-6},
  pagetotal = {269-302},
  keywords = {BJH method,Density functional theory,DFT method,Kelvin equation,Mesopores,NLDFT,Pore size analysis,Pore size distribution,QDFT},
  file = {C\:\\Users\\Jonathan\\Zotero\\storage\\6NHFRETS\\2013-Assessment_of_Mesoporosity.pdf}
}

