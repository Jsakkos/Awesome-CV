@article{shiHighlyMultiplexedSpatial2020,
  title = {Highly Multiplexed Spatial Mapping of Microbial Communities},
  author = {Shi, Hao and Shi, Qiaojuan and Grodner, Benjamin and Lenz, Joan Sesing and Zipfel, Warren R. and Brito, Ilana Lauren and De Vlaminck, Iwijn},
  date = {2020-12-24},
