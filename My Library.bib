@article{mooreMechanicalRegulationPhotosynthesis2020,
  title = {Mechanical Regulation of Photosynthesis in Cyanobacteria},
  author = {Moore, Kristin A. and Altus, Sabina and Tay, Jian W. and Meehl, Janet B. and Johnson, Evan B. and Bortz, David M. and Cameron, Jeffrey C.},
  date = {2020},
  journaltitle = {Nature Microbiology},
