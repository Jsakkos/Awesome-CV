@article{chenEfficientImmobilizationWhole2004,
  title = {Efficient Immobilization of Whole Cells of {{Methylomonas}} Sp. Strain {{GYJ3}} by Sol–Gel Entrapment},
  author = {Chen, Jianbo and Xu, Yi and Xin, Jiaying and Li, Shuben and Xia, Chungu and Cui, Junru},
