@article{Hwang2013,
  title = {Enhancement of the Spectral Selectivity of Complex Samples by Measuring Them in a Frozen State at Low Temperatures in Order to Improve Accuracy for Quantitative Analysis. {{Part I}}. {{Raman}} Spectroscopic Compositional Analysis of Synthetic Hydrocarbon Mixtures},
