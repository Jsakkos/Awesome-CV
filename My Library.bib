@incollection{singhGenerationStableLightDriven2022,
  title = {Generation of {{Stable}}, {{Light-Driven Co-cultures}} of {{Cyanobacteria}} with {{Heterotrophic Microbes}}},
  booktitle = {Plant {{Synthetic Biology}}: {{Methods}} and {{Protocols}}},
  author = {Singh, Amit K. and Ducat, Daniel C.},
