  author = {Pannier, Angela and Mkandawire, Martin and Soltmann, Ulrich and Pompe, Wolfgang and Böttcher, Horst},
  date = {2012-02},
  journaltitle = {Applied microbiology and biotechnology},
  volume = {93},
  number = {4},
