@article{orusBiogenesisUltrastructureCarboxysomes1995,
  title = {Biogenesis and {{Ultrastructure}} of {{Carboxysomes}} from {{Wild Type}} and {{Mutants}} of {{Synechococcus}} Sp. {{Strain PCC}} 7942.},
  author = {Orus, M. I. and Rodriguez, M. L. and Martinez, F. and Marco, E.},
