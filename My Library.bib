@article{Suk2016,
  title = {Environmental Pollution: {{An}} Underrecognized Threat to Children’s Health, Especially in Low- and Middle-Income Countries},
  author = {Suk, William A. and Ahanchian, Hamid and Asante, Kwadwo Ansong and Carpenter, David O. and Diaz-Barriga, Fernando and Ha, Eun Hee and Huo, Xia and King, Malcolm and Ruchirawat, Mathuros and da Silva, Emerson R. and Sly, Leith and Sly, Peter D. and Stein, Renato T. and van den Berg, Martin and Zar, Heather and Landrigan, Philip J.},
