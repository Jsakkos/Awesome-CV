@article{Aukema2014a,
  title = {Biodegradation of {{Novel Hydrocarbon Ring Structures Found}} in {{Hydraulic Fracturing Waters Using Silica-Encapsulated Pseudomonas}} Sp. {{NCIB}} 9816-4.},
  author = {Aukema, Kelly G and Kasinkas, Lisa and Aksan, Alptekin and Wackett, Lawrence P},
