@article{Fu2006,
  title = {Single Molecule Spectroscopy Studies of Diffusion in Mesoporous Silica Thin Films.},
  author = {Fu, Yi and Ye, Fangmao and Sanders, William G and Collinson, Maryanne M and a Higgins, Daniel},
