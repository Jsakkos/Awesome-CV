@article{sabriMolecularControlSucrose2013,
  title = {Molecular Control of Sucrose Utilization in {{Escherichia}} Coli {{W}}, an Efficient Sucrose-Utilizing Strain},
  author = {Sabri, Suriana and Nielsen, Lars K. and Vickers, Claudia E.},
  date = {2013-01},
  journaltitle = {Applied and Environmental Microbiology},
  volume = {79},
