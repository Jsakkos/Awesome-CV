@article{Rodahl1995,
  title = {On {{The Measurement Of Thin Liquid Overlayers With The}}  {{Quartz-crystal Microbalance}}},
  author = {Rodahl, M. and Kasemo, B.},
  date = {1995},
  journaltitle = {Proceedings of the International Solid-State Sensors and Actuators Conference - TRANSDUCERS '95},
